{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": 1,
    "no-debugger": 1,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "eol-last": 0,
    "quotes": 0,
    "no-empty": 1,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-var": 1,
    "default-case": 1,
    "require-await": 1,
    "comma-dangle": 1,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 1
  }
}
